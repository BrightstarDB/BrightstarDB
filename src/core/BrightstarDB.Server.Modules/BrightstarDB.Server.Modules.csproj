<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net462</TargetFramework>
        <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
        <RootNamespace>BrightstarDB.Server.Modules</RootNamespace>
        <AssemblyName>BrightstarDB.Server.Modules</AssemblyName>
    </PropertyGroup>
    
    <ItemGroup>
        <ProjectReference Include="..\BrightstarDB.Core\BrightstarDB.core.csproj" />
    </ItemGroup>
    
    <ItemGroup>
        <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
        <PackageReference Include="Nancy" Version="1.4.5" />
        <PackageReference Include="Nancy.Authentication.Basic" Version="1.4.1" />
    </ItemGroup>
    
    <ItemGroup>
    <Content Include="..\BrightstarDB.Server.AspNet\Views\**\*.*">
      <Link>Views\%(recursivedir)\%(filename)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="..\BrightstarDB.Server.AspNet\assets\**\*.*">
      <Link>assets\%(recursivedir)\%(filename)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <None Include="app.config" />
    <None Include="web.config" />
  </ItemGroup>

</Project>
